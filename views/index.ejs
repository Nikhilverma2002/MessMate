    <!-- ======= Hero Section ======= -->
    <section id="hero" class="d-flex align-items-center">
      <div class="container">
        <div class="row gy-4">
          <div
            class="col-lg-6 order-2 order-lg-1 d-flex flex-column justify-content-center"
          >
            <h1>
              Simplify your
              <span style="color: #eb5d1e">Mess-y</span> Management
            </h1>

            <h2>Enjoy a streamlined dining experience with MessMate</h2>
            <div>
              <a href="#contact" class="btn-get-started scrollto">Contact Us</a>
            </div>
          </div>
          <div class="col-lg-6 order-1 order-lg-2 hero-img">
            <img
              height="80%"
              width="75%"
              src="https://firebasestorage.googleapis.com/v0/b/do-it-5e954.appspot.com/o/mess_pic%20.png?alt=media&token=1f7a68f8-2c84-4d37-aef2-58fa32e3370e&_gl=1*14fs5s6*_ga*MTkyNDE4MzE1OC4xNjk2Nzg4ODA3*_ga_CW55HF8NVT*MTY5ODQ0MDIyOS4zLjEuMTY5ODQ0MDI2MC4yOS4wLjA."
              class="img-fluid animated"
              alt=""
            />
          </div>
        </div>
      </div>
    </section>
    <!-- End Hero -->

    <main id="main">
      <!-- ======= About Section ======= -->
      <section id="about" class="about">
        <div class="container">
          <div class="row justify-content-between">
            <div
              class="col-lg-5 d-flex align-items-center justify-content-center about-img"
            >
              <img
                width="80%"
                height="80%"
                src="https://firebasestorage.googleapis.com/v0/b/do-it-5e954.appspot.com/o/mess_pic%20%20(1).png?alt=media&token=6b9316f7-da8d-4548-834e-bfd8c2432796&_gl=1*grxkku*_ga*MTkyNDE4MzE1OC4xNjk2Nzg4ODA3*_ga_CW55HF8NVT*MTY5ODQ0MjI2My40LjEuMTY5ODQ0MjI4My40MC4wLjA."
                class="img-fluid"
                alt=""
                data-aos="zoom-in"
              />
            </div>
            <div class="col-lg-6 pt-5 pt-lg-0">
              <h3 data-aos="fade-up">What we do?</h3>
              <p data-aos="fade-up" data-aos-delay="100">
                MessMate revolutionizes mess management by providing a seamless
                platform connecting users with owners.
              </p>
              <div class="row">
                <div class="col-md-6" data-aos="fade-up" data-aos-delay="100">
                  <i class="bx"></i>
                  <img
                    style="margin-top: 15px"
                    height="20%"
                    width="20%"
                    src="../assets/img/service/1.svg"
                    style="margin-bottom: 20px"
                  />
                  <h4>Mess Owners</h4>
                  <p>
                    MessMate provides mess owners with a powerful suite of tools
                    to streamline operations. Manage transactions, track
                    attendance, and optimize resources effortlessly. Elevate
                    your mess management with MessMate's intuitive dashboard and
                    dedicated support.
                  </p>
                </div>
                <div class="col-md-6" data-aos="fade-up" data-aos-delay="200">
                  <i class="bx"></i>
                  <img
                    style="margin-top: 10px"
                    height="23%"
                    width="20%"
                    src="../assets/img/service/2.svg"
                    style="margin-bottom: 20px"
                  />
                  <h4>Mess Users</h4>
                  <p>
                    MessMate offers users a hassle-free way to connect with mess
                    owners. Enjoy cashless transactions, keep tabs on expenses,
                    and contribute to reducing food wastage with our convenient
                    attendance feature. Elevate your dining experience with
                    MessMate.
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- End About Section -->

      <!-- ======= Services Section ======= -->
      <section id="services" class="services section-bg">
        <div class="container" data-aos="fade-up">
          <div class="section-title">
            <h1>Services</h1>
            <p>Check out the great services we provide</p>
          </div>

          <div class="row">
            <div
              class="col-md-6 col-lg-3 d-flex align-items-stretch"
              data-aos="zoom-in"
              data-aos-delay="100"
            >
              <div class="icon-box">
                <div class="icon">
                  <i class="bx"></i
                  ><img
                    height="18%"
                    width="18%"
                    src="../assets/img/service/1.png"
                    style="margin-bottom: 20px"
                  />
                </div>
                <h4 class="title"><a href="">Admin Panel</a></h4>
                <p class="description">
                  Effortlessly manage menus, track orders, and gain valuable
                  insights to streamline operations.
                </p>
              </div>
            </div>

            <div
              class="col-md-6 col-lg-3 d-flex align-items-stretch"
              data-aos="zoom-in"
              data-aos-delay="200"
            >
              <div class="icon-box">
                <div class="icon">
                  <i class="bx"></i
                  ><img
                    height="18%"
                    width="18%"
                    src="../assets/img/service/3.png"
                    style="margin-bottom: 20px"
                  />
                </div>
                <h4 class="title"><a href="">Advertising</a></h4>
                <p class="description">
                  Advertise your culinary offerings on our platform and connect
                  with a large audience.
                </p>
              </div>
            </div>

            <div
              class="col-md-6 col-lg-3 d-flex align-items-stretch"
              data-aos="zoom-in"
              data-aos-delay="300"
            >
              <div class="icon-box">
                <div class="icon">
                  <i class="bx"></i
                  ><img
                    height="18%"
                    width="18%"
                    src="../assets/img/service/modern.png"
                    style="margin-bottom: 20px"
                  />
                </div>
                <h4 class="title"><a href="">Automated System</a></h4>
                <p class="description">
                  From automated ordering to instant receipts, elevate your
                  service with innovation.
                </p>
              </div>
            </div>

            <div
              class="col-md-6 col-lg-3 d-flex align-items-stretch"
              data-aos="zoom-in"
              data-aos-delay="400"
            >
              <div class="icon-box">
                <div class="icon">
                  <i class="bx"></i
                  ><img
                    height="18%"
                    width="18%"
                    src="../assets/img/service/cashless.png"
                    style="margin-bottom: 20px"
                  />
                </div>
                <h4 class="title"><a href="">Cashless Transaction</a></h4>
                <p class="description">
                  Enjoy a seamless and efficient way to manage your mess
                  finances & say goodbye to the hassle of handling cash.
                </p>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- End Services Section -->

      <!-- ======= Portfolio Section ======= -->
      <section id="portfolio" class="portfolio">
        <div class="container" data-aos="fade-up">
          <div class="section-title">
            <!-- <h2>Portfolio</h2> -->
            <p>Our Clients</p>
          </div>

          <div
            class="row portfolio-container"
            data-aos="fade-up"
            data-aos-delay="100"
          >
            <% if (messList && messList.length > 0) { %> <%
            messList.forEach(messSystem => { %>
            <div class="col-lg-4 col-md-6 portfolio-item filter-app">
              <div class="portfolio-wrap">
                <img
                  width="100%"
                  height="100px"
                  src="https://content.jdmagicbox.com/comp/bilaspur-chhattisgarh/b4/9999p7752.7752.220710141051.w8b4/catalogue/community-kitchen-bilaspur-city-bilaspur-chhattisgarh-tiffin-services-711qjd0qyx.jpg"
                  class="img-fluid"
                  alt=""
                />
                <div class="portfolio-links">
                  <a
                    href="https://content.jdmagicbox.com/comp/bilaspur-chhattisgarh/b4/9999p7752.7752.220710141051.w8b4/catalogue/community-kitchen-bilaspur-city-bilaspur-chhattisgarh-tiffin-services-711qjd0qyx.jpg"
                    data-gallery="portfolioGallery"
                    class="portfolio-lightbox"
                    title="Sai mess food cornor"
                    ><i class="bi bi-plus"></i
                  ></a>
                  <a href="portfolio-details.html" title="More Details"
                    ><i class="bi bi-link"></i
                  ></a>
                </div>
                <div class="portfolio-info">
                  <h4><%= messSystem.messName %></h4>
                  <p><%= messSystem.location %></p>
                </div>
              </div>
            </div>
            <% }) %> <% } else { %>
            <p>No data available</p>
            <% } %>
          
        </div>
      </section>
      <!-- End Portfolio Section -->

